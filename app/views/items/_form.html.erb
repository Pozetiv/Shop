<%= form_for @item, html: { multipart: true } do |f|  %>

<div class="form-group">
  <%= f.label :title %>
  <%= f.text_field :title, class: 'form-control' %>
</div>

<div class="form-group">
  <%= f.label :description %>
  <%= f.text_area :description, class: 'form-control' %>
</div>

    <div class="form-grup">
<%= f.label :price %>
<%= f.text_field :price %>
    </div>
<div class="form-group">
  <%= f.file_field :image %>
</div>
<%= f.submit %>
<% end %>